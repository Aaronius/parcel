{
  "bundler": "@parcel/bundler-default",
  "transforms": {
    "*.{js,mjs,jsm,jsx,es6}": [
      "@parcel/transformer-babel",
      "@parcel/transformer-js",
      "@parcel/transformer-terser"
    ],
    "*.css": ["@parcel/transformer-css"]
  },
  "packagers": {
    "*.js": "@parcel/packager-js"
  },
  "resolvers": ["@parcel/resolver-default"]
}
